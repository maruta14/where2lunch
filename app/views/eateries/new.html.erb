<%= form_with model: Eatery.new, url: {action: :create},
local: true do |f| %>
<div>
   <%= f.label :name %>
   <%= f.text_field :name, id: 'eatery_name' %>
</div>
<%= f.submit %>
<% flash.each do |message_type, message| %>
     <div class="alert alert-<%= message_type %>"><%= message %></div>
<% end %>
<%= yield %>
<% end %>
